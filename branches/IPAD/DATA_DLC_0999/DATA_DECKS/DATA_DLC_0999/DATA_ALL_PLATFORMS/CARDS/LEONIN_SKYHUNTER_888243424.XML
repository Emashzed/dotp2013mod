    <LOCALISED_TEXT LanguageCode="en-US"><![CDATA[Leonin Skyhunter]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="it-IT"><![CDATA[Solcacielo Leonid]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="de-DE"><![CDATA[Leoniden-Himmeljäger]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="fr-FR"><![CDATA[Chasseciel léonin]]></LOCALISED_TEXT>
