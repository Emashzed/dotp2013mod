    <LOCALISED_TEXT LanguageCode="es-ES"><![CDATA[Peregrino de Avacyn]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="jp-JA"><![CDATA[アヴァシンの巡礼者]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="ko-KR"><![CDATA[Avacyn’s Pilgrim]]></LOCALISED_TEXT>
    <LOCALISED_TEXT LanguageCode="ru-RU"><![CDATA[Пилигрим Авацины]]></LOCALISED_TEXT>
